  },
  "homepage": "https://github.com/vasanthk/react-es6-webpack-boilerplate",
  "dependencies": {
    "babel-runtime": "^6.3.19",
    "lodash": "^4.11.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "babel-polyfill": "^6.3.14",
    "isomorphic-fetch": "^2.1.1",
    "react-redux": "^4.2.1",
    "redux": "^3.2.1",
